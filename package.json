{
  "name": "dash_echarts",
  "version": "0.1.1.1",
  "description": "echarts for dash",
  "repository": {
    "type": "git",
    "url": "git://github.com/mergeforward/dash-echarts.git"
  },
  "bugs": {
    "url": "https://github.com/mergeforward/dash-echarts/issues"
  },
  "homepage": "https://github.com/mergeforward/dash-echarts",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-serve --config ./webpack.serve.config.js --open",
    "validate-init": "python _validate_init.py",
    "prepublishOnly": "npm run validate-init",
    "build:js": "webpack --mode production",
    "build:backends": "dash-generate-components ./src/lib/components dash_echarts -p package-info.json --r-prefix '' --jl-prefix ''",
    "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
    "build": "npm run build:js && npm run build:backends",
    "build:activated": "npm run build:js && npm run build:backends-activated"
  },
  "author": "dameng <pingf0@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "echarts": "^5.5.1",
    "echarts-gl": "^2.0.9",
    "echarts-stat": "^1.2.0",
    "mapbox-gl": "^3.8.0",
    "ramda": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^9.1.3",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "npm": "^10.2.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-docgen": "^7.0.1",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.3",
    "styled-jsx": "^5.1.2",
    "webpack": "^5.96.1",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  }
}
